<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory android:title="常用設置類">
        <CheckBoxPreference
            android:title="通知"
            android:defaultValue="true"
            android:key="notify"
            android:summary="啟用通知服務"/>
        <ListPreference
            android:title="幣別"
            android:key="currency"
            android:summary="租金使用的幣別"
            android:entries="@array/currency"
            android:entryValues="@array/currencyKey"/>
    </PreferenceCategory>

</PreferenceScreen>